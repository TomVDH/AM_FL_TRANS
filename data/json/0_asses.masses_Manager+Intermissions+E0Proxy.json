        },
        {
          "rowNumber": 5,
          "utterer": "WRITE.DialogLocalizer.12",
          "context": "",
          "sourceEnglish": "Which does your society value more?"
        },
        {
          "rowNumber": 6,
          "utterer": "WRITE.DialogLocalizer.13",
          "context": "",
          "sourceEnglish": "Those who stay with the herd"
        },
        {
          "rowNumber": 7,
          "utterer": "WRITE.DialogLocalizer.14",
          "context": "",
          "sourceEnglish": "Those who step away from the herd"
        },
        {
          "rowNumber": 8,
          "utterer": "WRITE.DialogLocalizer.53",
          "context": "",
          "sourceEnglish": "Do you think of yourself as an intelligent animal?"
        },
        {
          "rowNumber": 9,
          "utterer": "WRITE.DialogLocalizer.55",
          "context": "",
          "sourceEnglish": "Yes"
        },
        {
          "rowNumber": 10,
          "utterer": "WRITE.DialogLocalizer.54",
          "context": "",
          "sourceEnglish": "No"
        },
        {
          "rowNumber": 11,
          "utterer": "WRITE.DialogLocalizer.56",
          "context": "",
          "sourceEnglish": "Do you think of yourself as a political animal?"
        },
        {
          "rowNumber": 12,
          "utterer": "WRITE.DialogLocalizer.57",
          "context": "",
          "sourceEnglish": "Yes"
        },
        {
          "rowNumber": 13,
          "utterer": "WRITE.DialogLocalizer.58",
          "context": "",
          "sourceEnglish": "No"
        },
        {
          "rowNumber": 14,
          "utterer": "WRITE.DialogLocalizer.59",
          "context": "",
          "sourceEnglish": "Do you have urges to copulate and reproduce?"
        },
        {
          "rowNumber": 15,
          "utterer": "WRITE.DialogLocalizer.60",
          "context": "",
          "sourceEnglish": "Yes"
        },
        {
          "rowNumber": 16,
          "utterer": "WRITE.DialogLocalizer.61",
          "context": "",
          "sourceEnglish": "No"
        },
        {
          "rowNumber": 17,
          "utterer": "WRITE.DialogLocalizer.62",
          "context": "",
          "sourceEnglish": "What is your natural habitat?"
        },
        {
          "rowNumber": 18,
          "utterer": "WRITE.DialogLocalizer.63",
          "context": "",
          "sourceEnglish": "Grassy plains"
        },
        {
          "rowNumber": 19,
          "utterer": "WRITE.DialogLocalizer.140",
